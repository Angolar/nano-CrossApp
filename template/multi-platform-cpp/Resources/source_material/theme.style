        <Color titleColor_selected="0xffffffff"/>
        <Color tintColor="0xff36C3F0"/>
        <Size height="60"/>
    </CASegmentedControl>
    <CAActivityIndicatorView name="CAActivityIndicatorView">
        <Image image_white="source_material/loading_white.png"/>
        <Image image_gray="source_material/loading_black.png"/>
