        <BackgroundView backgroundView_selected_round="source_material/page_round_h.png"/>
        <BackgroundView backgroundView_normal_rect="source_material/page_rect_n.png"/>
        <BackgroundView backgroundView_selected_rect="source_material/page_rect_h.png"/>
    </CAPageControl>
    <CANavigationBar name="CANavigationBar">
        <BackgroundView backgroundView="source_material/navigation_bg.png"/>
        <Image leftButtonImage="source_material/btn_left_white.png"/>
    </CANavigationBar>
    <CABadgeView name="CABadgeView">
        <Image badgeImage="source_material/bg_badge.png"/>
    </CABadgeView>
    <CACell name="CACell">
        <BackgroundColor backgroundColor_normal="0xffffffff"/>
        <BackgroundColor backgroundColor_highlighted="0xffF0F0F0"/>
        <BackgroundColor backgroundColor_selected="0xff32C1FF"/>
        <BackgroundColor backgroundColor_disabled="0xff7F7F7F"/>
        <Color separatorColor="0xffefeef4"/>
    </CACell>
