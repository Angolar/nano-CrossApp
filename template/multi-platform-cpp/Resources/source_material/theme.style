    </CAProgress>
    <CASlider name="CASlider">
        <Image minTrackTintImage="source_material/slider_min_track.png"/>
        <Image maxTrackTintImage="source_material/slider_max_track.png"/>
        <Image thumbTintImage="source_material/slider_indicator.png"/>
        <Size trackHeight="4"/>
        <Size height="36"/>
    </CASlider>
    <CASwitch name="CASwitch">
        <Image onImage="source_material/switch_on.png"/>
        <Image offImage="source_material/switch_off.png"/>
        <Image thumbTintImage="source_material/switch_indicator.png"/>
        <Size width="102"/>
        <Size height="56"/>
    </CASwitch>
    <CAStepper name="CAStepper">
        <BackgroundView backgroundView_normal="source_material/stepper_normal.png"/>
        <BackgroundView backgroundView_selected="source_material/stepper_selected.png"/>
        <Image decImage_normal="source_material/stepper_dec_n.png"/>
        <Image decImage_highlighted="source_material/stepper_dec_h.png"/>
        <Image incImage_normal="source_material/stepper_inc_n.png"/>
        <Image incImage_highlighted="source_material/stepper_inc_h.png"/>
        <Color tintColor="0xff36C3F0"/>
    </CAStepper>
    <CATextField name="CATextField">
        <BackgroundView backgroundView_normal="source_material/textField_bg.png"/>
        <Size height="80"/>
    </CATextField>
    <CATabBar name="CATabBar">
