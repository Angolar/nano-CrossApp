<?xml version="1.0" encoding="UTF-8"?>
<Theme>
    <CAButton name="CAButton_SquareRect">
        <BackgroundView backgroundView_normal="source_material/btn_square_normal.png"/>
        <BackgroundView backgroundView_highlighted="source_material/btn_square_highlighted.png"/>
        <BackgroundView backgroundView_selected="source_material/btn_square_selected.png"/>
        <BackgroundView backgroundView_disabled="source_material/btn_square_disabled.png"/>
        <Color titleColor_normal="0xff2ec0ff"/>
        <Color titleColor_highlighted="0xffffffff"/>
        <Color titleColor_selected="0xffffffff"/>
        <Color titleColor_disabled="0xffffffff"/>
        <Size height="60"/>
    </CAButton>
    <CAButton name="CAButton_RoundedRect">
        <BackgroundView backgroundView_normal="source_material/btn_rounded_normal.png"/>
        <BackgroundView backgroundView_highlighted="source_material/btn_rounded_highlighted.png"/>
        <BackgroundView backgroundView_selected="source_material/btn_rounded_selected.png"/>
        <BackgroundView backgroundView_disabled="source_material/btn_rounded_disabled.png"/>
        <Color titleColor_normal="0xff2EC0FF"/>
        <Color titleColor_highlighted="0xffffffff"/>
        <Color titleColor_selected="0xffffffff"/>
        <Color titleColor_disabled="0xffffffff"/>
        <Size height="60"/>
